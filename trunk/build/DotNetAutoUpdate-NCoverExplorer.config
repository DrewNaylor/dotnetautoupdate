<?xml version="1.0" encoding="utf-8"?>
<ConsoleSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<ProjectName>DotNetAutoUpdate</ProjectName>
	<SatisfactoryCoverageThreshold>100</SatisfactoryCoverageThreshold>
	<CoverageFileNames>
		<CoverageFileName>coverage.xml</CoverageFileName>
	</CoverageFileNames>
	<FailIfBelowMinimum>true</FailIfBelowMinimum>
  
    <CoverageExclusions>
        <CoverageExclusion>
            <ExclusionType>Assembly</ExclusionType>
            <Pattern>MvcToolkit</Pattern>
        </CoverageExclusion>
    </CoverageExclusions>
</ConsoleSetting>